/*
This program asks the user to input a number and then it says whether the number is
a multiple of five or two.
*/


import java.util.Scanner;
  
public class Welcome {
    public static void main(String[] args){
    Scanner input= new Scanner(System.in);
    
    System.out.print("Enter a number ");
    int number =input.nextInt();
    
    if(number%5==0) {
        System.out.println("The number is a multiple of five");
        
    
         if(number %2==0) 
       System.out.println("The number is a multiple of two");
   }    
             
   else
       System.out.println("Negative input");
   
    }         
}
